    <!-- Modal Notify -->
    <div class="modal fade" id="notify" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog notify-modal">
            <div class="modal-content notify-content">
                <div class="modal-header notify-header">
                    <h1 class="modal-title fs-3 text-danger fw-bold" id="exampleModalLabel">Thông báo</h1>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body notify-body">
                    <div class="notifications">
                        @if (ViewBag.NotifyUser != null)
                        {
                            foreach (var notify in (List<accgame.Context.NotifyUser>)ViewBag.NotifyUser)
                            {
                                if (notify.IsAdminPost == true)
                                {
                                    <div class="notify-item p-3 mb-3 border-bottom bg-light border border-primary">
                                        <h6 class="text-danger">Thông báo từ Admin:</h6>
                                        @Html.Raw(notify.Contents) <!-- Render nội dung HTML -->
                                        <p class="text-end text-primary"><i>@notify.Thoigian</i></p>
                                    </div>
                                }
                                else
                                {
                                    <div class="notify-item p-3 mb-3 border-bottom bg-light border border-primary">
                                        @Html.Raw(notify.Contents) <!-- Render nội dung HTML -->
