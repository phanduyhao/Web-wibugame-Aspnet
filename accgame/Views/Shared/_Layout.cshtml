        function toggleNavbar2() {
            const navbar2 = document.querySelector('#navbar2');
            const navbarToggle2 = document.querySelector('.mobile-nav-toggle');

            navbar2.classList.toggle('navbar-mobile');

            if (navbarToggle2) {
                navbarToggle2.classList.toggle('bi-list');
                navbarToggle2.classList.toggle('bi-x');
            }
        }
        const navbar2 = document.querySelector('#navbar2');
        const menu = document.querySelector('#mobile-nav-trigger2');
        menu.addEventListener('click', function () {
            if (navbar2.classList.contains('d-none')) {
                navbar2.classList.remove('d-none')
                navbar2.classList.add('d-block')

            } else {
                navbar2.classList.add('d-none')
                navbar2.classList.remove('d-block')

            }
        });
        document.querySelector('#mobile-nav-trigger2').addEventListener('click', toggleNavbar2);

        document.addEventListener('click', function (e) {
            if (e.target.matches('.navbar .dropdown > a') && document.querySelector('#navbar2').classList.contains('navbar-mobile')) {
                e.preventDefault();
                e.target.nextElementSibling.classList.toggle('dropdown-active');
            }
        }, true);

        document.addEventListener('click', function (e) {
            const navbar2 = document.querySelector("#navbar2");
            navbar2.classList.remove("mx-auto");
            navbar2.classList.add("me-auto");
            if (e.target.matches('.scrollto')) {
                if (document.querySelector(e.target.hash)) {
                    e.preventDefault();

                    const navbar2 = document.querySelector('#navbar2');
                    if (navbar2.classList.contains('navbar-mobile')) {
                        navbar2.classList.remove('navbar-mobile');
                        navbar2.classList.add('d-none');
                        const navbarToggle2 = document.querySelector('.mobile-nav-toggle');
                        if (navbarToggle2) {
                            navbarToggle2.classList.toggle('bi-list');
                            navbarToggle2.classList.toggle('bi-x');
                        }
                    }
                    scrollto(e.target.hash);
                }
            }
        }, true);

